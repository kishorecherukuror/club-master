      <div class="carousel-inner">
        <div class="item active">
          <img src="assets/i1.jpg" alt="First slide">
          <div class="container">
            <div class="carousel-caption">
              
              <p><%= link_to 'Go For Coffee', {:action=>"new", :member_type => 'coffee', :controller => 'club'}, :class => 'btn btn-lg btn-primary' %></p>
            </div>
          </div>
        </div>
        <div class="item">
          <img src="assets/i2.jpg" alt="Second slide">
